# Save exact dependencies
save-exact=true

# Use package-lock.json
package-lock=true

# Set default registry
registry=https://registry.npmjs.org/

# Set log level
loglevel=warn

# Set progress to false for CI environments
progress=false

# Set color to false for CI environments
color=false

# Set fund to false to disable funding message
fund=false

# Set audit-level to moderate
audit-level=moderate